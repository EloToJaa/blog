---
import Header from "../components/navbar/Header.astro";
import MainHead from "./MainHead.astro";

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<html lang="en">
  <head>
    <MainHead title={title} />
  </head>
  <body>
    <div class="container mx-auto font-main">
      <Header />
      <main class="mt-3">
        <slot />
      </main>
    </div>
  </body>
</html>
